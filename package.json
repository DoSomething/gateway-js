{
  "name": "@dosomething/gateway",
  "version": "1.3.0",
  "description": "Standard JS API client for DoSomething.org services.",
  "module": "es/gateway.js",
  "main": "lib/gateway.js",
  "babel": {
    "presets": [
      "@dosomething/babel-config"
    ]
  },
  "sideEffects": false,
  "engines": {
    "node": "8.11.1",
    "npm": "5.7.1"
  },
  "scripts": {
    "build": "npm run build:es && npm run build:cjs",
    "build:es": "NODE_ENV=test babel src --out-dir=es",
    "build:cjs": "NODE_ENV=test babel src --out-dir=lib",
    "prepublish": "npm run build"
  },
  "author": "Diego Lorenzo <dlorenzo@dosomething.org>",
  "license": "MIT",
  "dependencies": {
    "camelcase": "^5.0.0",
    "date-fns": "^1.29.0",
    "lodash": "^4.17.4",
    "simple-oauth2": "^1.5.2",
    "superagent": "^3.8.3",
    "superagent-use": "^0.1.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@dosomething/babel-config": "^2.1.0",
    "@dosomething/eslint-config": "^4.0.0",
    "@dosomething/webpack-config": "^1.1.1",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.3"
  }
}
